{
	"name": "@matise/ui-fields",
	"version": "0.8.1",
	"description": "uiFields generator for Vue",
	"author": "Dennis Dipsaus",
	"private": false,
	"main": "dist/ui-fields.umd.js",
	"module": "dist/ui-fields.esm.js",
	"scripts": {
		"build": "npm run build:es & npm run build:umd",
		"build:umd": "rollup --config build/rollup.config.js --format umd --file dist/ui-fields.umd.js",
		"build:es": "rollup --config build/rollup.config.js --format es --file dist/ui-fields.esm.js",
		"build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/ui-fields.min.js"
	},
	"devDependencies": {
		"babel-eslint": "^10.0.1",
		"eslint": "^5.0.1",
		"eslint-config-prettier": "^3.1.0",
		"eslint-friendly-formatter": "^4.0.1",
		"eslint-loader": "^2.1.1",
		"eslint-plugin-prettier": "^2.7.0",
		"eslint-plugin-vue": "^4.7.1",
		"henris": "^0.4.2",
		"minimist": "^1.2.0",
		"prettier": "^1.14.3",
		"rollup": "^0.57.1",
		"rollup-plugin-buble": "^0.19.2",
		"rollup-plugin-uglify-es": "0.0.1",
		"rollup-plugin-vue": "^4.3.2",
		"vue": "^2.5.16",
		"vue-template-compiler": "^2.5.16"
	}
}
